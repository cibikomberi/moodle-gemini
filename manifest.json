{
  "name": "moodle-gemini",
  "version": "2.0",
  "description": "answers for moodle",
  "manifest_version": 3,
  "action": {
    "default_popup": "front.html",
    "default_name": "moodle cracker"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs",
    "identity",
    "identity.email",
    "cookies",
    "webRequest",
    "declarativeNetRequestWithHostAccess"
  ],
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ],
      "matches": [
        "https://moodle.bitsathy.ac.in/*","https://janaticselearning.com/*"
      ]
    }
  ],
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "gen.js",
        "main_working.js",
        "iframer.html",
        "https://chatgpt.com"
      ]
    }
  ],
  "background": {"service_worker": "bg.js"}
}