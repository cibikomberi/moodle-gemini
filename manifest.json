{
    "name": "moodle-gemini",
    "version": "2.0",
    "description": "answers for moodle",
    "manifest_version": 3,
    "update_url": "https://github.com/cibivishnukomberi/moodle-gemini/updates.xml",
    "action":{
        "default_popup": "index.html",
        "default_name": "moodle cracker"
    },"permissions": ["activeTab", "scripting","storage","tabs",
    "idle",
    "identity",
    "identity.email"],
        "content_scripts": [
          {
            "js": ["content.js"],
            "css": ["content.css"],
            "module":true,
            "matches": [
              "https://moodle.bitsathy.ac.in/*"
            ]
          }
        ],
        "web_accessible_resources": [{
          "matches": ["<all_urls>"],
          "resources": ["https://esm.run/@google/generative-ai",
                      "https://github.com/cibivishnukomberi/moodle-gemini/blob/main/manifest.json",
                      "main_working.js"]
        }]
      
}